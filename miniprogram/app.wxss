page{
  width: 100%;
}
  
.titleContainer{
  width: 100%;
  padding: 100rpx;
}
.big-title {
  color: #337ab7;
  font-size: 44rpx;
}


.input-container {
  display: flex;
  flex-direction: column;
  align-items: center;
  flex-wrap: wrap;
  background-color: #fcfcfc;
  width: 720rpx;
  margin-left: 15rpx;
  margin-right: 15rpx;
  font-size: 44rpx;
  box-sizing: border-box;
  box-shadow: 4px 1px 1px #ccc;
  position: relative;
  padding-bottom: 100rpx;
}

.btn {
  position: absolute;
  right: 5%;
  bottom: 5%;
  width: 160rpx!important;
  font-size: 26rpx;
  border-radius: 10rpx;
  background: #337ab7;
  color: #fff;
  padding: 0!important;
  min-height: 30rpx!important;
  height: 60rpx;
  line-height: 60rpx;
}

.algo-title {
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  font-size: 36rpx;
}

.line {
  width: 720rpx;
  height: 3rpx;
  background: rgba(0, 0, 0, 0.096);
}

.input-item {
  display: flex;
  flex-direction: row;
  justify-content: space-evenly;
  align-items: center;
  margin-top: 30rpx;
  height: 100rpx;
  width: 720rpx;
}

.param-name {
  font-size: 32rpx;
  width: 300rpx;
}

.input {
  width: 300rpx;
  height: 100rpx;
  border: 1px solid rgba(0, 0, 0, 0.096);
  border-radius: 25rpx;
}

